n        grant_type:'password',\r\n        username:this.email,\r\n        password:this.password\r\n      }\r\n\r\n      this.$http.post(\"oauth/token\",data).then( (response) =>{\r\n        //console.log(response);\r\n\r\n        if(response.ok){\r\n          this.$auth.setToken(response.body.access_token,response.body.expires_in + Date.now())\r\n\r\n          this.$http.get(\"api/user\",data).then(response =>{            \r\n            this.$auth.setUserId(response.body.id)\r\n             this.$router.push(\"/feed\")\r\n          })           \r\n        }\r\n        else{\r\n          alert('Datos incorrectos')\r\n        }\r\n        \r\n      })\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style>\r\n#app {\r\n  background-image: url(\"https://images.unsplash.com/photo-1497733942558-e74c87ef89db?dpr=1&auto=compress,format&fit=crop&w=1650&h=&q=80&cs=tinysrgb&crop=\");\r\n  background-size: cover;\r\n  overflow:hidden;\r\n}\r\n.loginOverlay {\r\n  background:rgba(33,150,243,0.3);\r\n}\r\n.photoCredit{\r\n  position: absolute;\r\n  bottom: 15px;\r\n  right: 15px;\r\n}\r\n</style>"]}]}